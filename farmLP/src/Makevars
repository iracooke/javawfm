PLATFORM=darwin

JAVA_LIBS=-framework JavaVM
JAVA_CPPFLAGS=-I/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Home/include
JAVA_HOME=/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Home
JAVA=/usr/bin/java
JAVAC=/usr/bin/javac
JAVAH=/usr/bin/javah

# Directory with COIN header files
COININCDIR = ./depends/coin-Cbc/include/

# Directory with COIN libraries
COINLIBDIR = ./depends/coin-Cbc/lib/

CPPFLAGS+=-Iinclude -I/System/Library/Frameworks/JavaVM.framework/Versions/1.5.0/Home/include -I$(COININCDIR)
CXX=g++

RINSTDIR=../inst/

include Makevars.$(PLATFORM)
